{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "Tfg"
  },
  "apps": [
    {
      "root": "client",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        {
          "glob": "**/*",
          "input": "../node_modules/leaflet/dist/images",
          "output": "./assets/"
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/leaflet/dist/leaflet.css",
        "../node_modules/leaflet-easybutton/src/easy-button.css",
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/leaflet/dist/leaflet.js",
        "../node_modules/leaflet-omnivore/leaflet-omnivore.min.js",
        "../node_modules/leaflet-easybutton/src/easy-button.js",
        "../node_modules/leaflet.heat/dist/leaflet-heat.js",
        "../node_modules/socket.io-client/dist/socket.io.js",
        "../client/app/core/services/leaflet-slider.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "production": "environments/environment.prod.ts",
        "staging": "environments/environment.staging.ts",
        "test": "environments/environment.test.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "client/tsconfig.app.json"
    },
    {
      "project": "client/tsconfig.spec.json"
    },
    {
      "project": "client/e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
